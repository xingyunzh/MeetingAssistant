<div class="container">
    <div id="Time" class="row">
        <div class="col-xs-2">
            <select class="form-control input-sm">
                <option>2016</option>
                <option>2017</option>
                <option>2018</option>
                <option>2019</option>
                <option>2020</option>
            </select>
        </div>
        <div class="col-xs-2">
            <select class="form-control input-sm" name="month">
                <option value="month">月</option>
                <option value="jan">1</option>
                <option value="febr">2</option>
                <option value="mar">3</option>
                <option>4</option>
                <option>5</option>
                <option>6</option>
                <option>7</option>
                <option>8</option>
                <option>9</option>
                <option>10</option>
                <option>11</option>
                <option>12</option>
            </select>
        </div>
        <div class="col-xs-2">
            <select class="form-control input-sm" name="day">
                <option value="day">日</option>
            </select>
        </div>
        <div class="col-xs-2">
            <select class="form-control input-sm" name="hour">
                <option value="hour">时</option>
            </select>
        </div>
        <div class="col-xs-2">
            <select class="form-control input-sm" name="minite">
                <option value="minite">分</option>
            </select>
        </div>
        <div class="col-lg-2">
            <div class="input-group">
               <span class="input-group-btn">
                  <button class="btn btn-default" type="button">
                     邮件提醒
                  </button>
               </span>
                <input type="text" class="form-control">
            </div><!-- /input-group -->
        </div><!-- /.col-lg-6 --><br>
    </div>
    <br>
    <div id="Details" class="row">
        <div class="col-xs-3">
            <input type="text" class="form-control" placeholder="会议名称">
        </div>
        <div class="col-xs-4">
            <input type="text" class="form-control" placeholder="会议地点">
        </div>
        <div class="col-xs-5">
            <input type="text" class="form-control" placeholder="会议描述">
        </div>
    </div>
    <br>
    <div class="form-group row" id="exactTime">
        <label for="dtp_input1" class="col-md-2 control-label">MeetingTime Picking</label>
        <div class="input-group date form_datetime col-md-5" data-date="1979-09-16T05:25:07Z" data-date-format="dd MM yyyy - HH:ii p" data-link-field="dtp_input1">
            <input class="form-control" size="16" type="text" value="" readonly>
            <span class="input-group-addon"><span class="glyphicon glyphicon-remove"></span></span>
            <span class="input-group-addon"><span class="glyphicon glyphicon-th"></span></span>
        </div>
        <input type="hidden" id="dtp_input1" value="" /><br/>
    </div>
    <table class="table" id="table">
        <caption>会议项目</caption>
        <thead>
        <tr>
            <th style="width: 5%" class="text-center">#</th>
            <th style="width: 10%" class="text-center">序号</th>
            <th style="width: 40%" class="text-center">内容</th>
            <th style="width: 10%" class="text-center">时间</th>
            <th style="width: 35%" class="text-center">关键问题</th>
        </tr>
        </thead>
        <tbody>
        <tr>
            <td><input type="checkbox" style="width: 100%" onclick="Select(event)"></td>
            <td><input type="text" style="width: 100%"></td>
            <td><input type="text" style="width: 100%"></td>
            <td><input type="text" style="width: 100%"></td>
            <td><input type="text" style="width: 100%"></td>
        </tr>
        </tbody>
    </table>
</div>
<div style="text-align:center">
    <button onclick="addRow()" class="btn btn-success">添加项目</button>
    <button id="delect" class="btn btn-danger">删除项目</button>
</div>